bestlasso <- glmnet(x,y,alpha=1,lambda=best.ll); coef(bestlasso)
r2.lasso <- rsq(bestlasso, best.ll, x, y)
cat("R-Squared :", r2.lasso)
rsq <- data.frame(Dummy = r2.dummy, WLS = r2.wls, Ridge = r2.ridge,
Lasso = r2.lasso)
